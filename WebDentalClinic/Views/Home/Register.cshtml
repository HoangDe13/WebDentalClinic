@model WebDentalClinic.Models.TAIKHOANBENHNHAN
@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section css{
    <link rel="stylesheet" type="text/css" href="~/Content/Contact.css" />
}

<div class="container contact-form">

    <h2 style="color:white">Đăng ký</h2>
    <hr>
    <div class="col-md-6">
        @using (Html.BeginForm("Register", "Home", FormMethod.Post))
        {
            //@Html.ValidationSummary()
            @Html.AntiForgeryToken()
            <div class="form-group">
                @Html.LabelFor(m => m.MaBenhNhan)
                @Html.TextBoxFor(m => m.MaBenhNhan, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(m => m.MaBenhNhan, "", new { @class = "badge badge-danger" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.TenTaiKhoan)
                @Html.TextBoxFor(m => m.TenTaiKhoan, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(m => m.TenTaiKhoan, "", new { @class = "badge badge-danger" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.MatKhau)
                @Html.PasswordFor(m => m.MatKhau, new { @class = "form-control" })
                @Html.ValidationMessageFor(m => m.MatKhau, "", new { @class = "badge badge-danger" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.XacNhanMatKhau)
                @Html.PasswordFor(m => m.XacNhanMatKhau, new { @class = "form-control" })
                @Html.ValidationMessageFor(m => m.XacNhanMatKhau, "", new { @class = "badge badge-danger" })
            </div>
            <div class="form-group">
                <input type="submit" name="submit" class="btn btn-danger" value="Register" />
            </div>
        }
    </div>
</div>

