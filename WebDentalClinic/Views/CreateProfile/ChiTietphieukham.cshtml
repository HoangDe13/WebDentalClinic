@model IEnumerable<WebDentalClinic.Models.CHITIETPHIEUKHAM>

@{
    ViewBag.Title = "ChiTietphieukham";
    Layout = "~/Views/Shared/Layout_Nurse.cshtml";
}
<div class="container body-content">
    <h2 style="font-family:'Times New Roman';text-align:center;">Chi Tiết Phiếu Khám</h2>

    <p>
        @Html.ActionLink("Danh Sách Phiếu Khám", "index",null,new { @class="btn btn-info"})
    </p>
    <table class="table">
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.DICHVU.TenDichVu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoLuong)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GhiChu)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.DICHVU.TenDichVu)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SoLuong)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GhiChu)
                </td>

            </tr>
        }

    </table>
    </div>